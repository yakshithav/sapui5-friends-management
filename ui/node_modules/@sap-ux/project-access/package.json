  "devDependencies": {
    "@types/mem-fs": "1.1.2",
    "@types/mem-fs-editor": "7.0.1",
    "@types/semver": "7.5.2",
    "@ui5/manifest": "1.66.0",
    "vscode-uri": "3.0.7",
    "@sap-ux/logger": "0.7.0"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rimraf --glob dist coverage *.tsbuildinfo",
