{
  "name": "@sap/bas-sdk",
  "version": "3.11.6",
  "description": "SDK for SAP Business Application Studio",
  "license": "SAP",
  "main": "dist/src/index.js",
  "typings": "./api.d.ts",
  "files": [
    "dist",
    "src",
    "api.d.ts"
  ],
  "scripts": {
    "ci": "npm-run-all clean compile coverage",
