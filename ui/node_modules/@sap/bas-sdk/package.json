    "url-join": "4.0.1",
    "ws": "8.17.1"
  },
  "devDependencies": {
    "@sap/bas-sdk-sinon-helper": "^3.11.6",
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "4.17.1",
    "@types/proxyquire": "^1.3.28",
    "@types/url-join": "4.0.3",
    "@types/uuid": "9.0.8",
    "@types/vscode": "^1.83.0",
    "@types/ws": "8.5.10",
