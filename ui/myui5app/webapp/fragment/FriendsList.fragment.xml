<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core">
  <Table items="{friendsModel>/friends}">
    <columns>
      <Column><Text text="Name"/></Column>
      <Column><Text text="City"/></Column>
      <Column><Text text="Education"/></Column>
      <Column><Text text="Action"/></Column>
    </columns>
    <items>
      <ColumnListItem>
        <cells>
          <Text text="{friendsModel>name}"/>
          <Text text="{friendsModel>city}"/>
          <Text text="{friendsModel>education}"/>
          <Button icon="sap-icon://delete" press=".onDeleteFriend"/>
        </cells>
      </ColumnListItem>
    </items>
  </Table>
</core:FragmentDefinition>
